{
  "compilerOptions": {
    "strict": true,
    "module": "commonjs",
    "allowJs": true,
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ESNext",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitAny": true,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "noUnusedLocals": true,
    "esModuleInterop": true,
    "types": [
      "node"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "paths": {
      "@api-main": [
        "apps/api-main"
      ],
      "@api-main/*": [
        "apps/api-main/*"
      ],
      "@libs/core": [
        "libs/core"
      ],
      "@libs/core/*": [
        "libs/core/*"
      ],
      "@libs/common": [
        "libs/common"
      ],
      "@libs/common/*": [
        "libs/common/*"
      ],
      "@libs/entities": [
        "libs/entities"
      ],
      "@libs/entities/*": [
        "libs/entities/*"
      ],
      "@libs/shared": [
        "libs/shared"
      ],
      "@libs/shared/*": [
        "libs/shared/*"
      ]
    }
  },
}
